package com.cap.np1;
 class Source {
	 String name;
	 int age;
	 int rn;
 
 public Source( String name,int age,int rn) {
	 this.name=name;
	 this.age=age;
	 this.rn=rn;
	 
 }
 public String getName() {
	 return this.name;
 }
 public void setName( String name) {
	 this.name=name; 
 }
 public int getAge() {
	 return this.age;
 }
 public void setAge( int age) {
	 this.age=age; 
 }
 public int getRn() {
	 return this.rn;
 }
 public void setRn( int rn) {
	 this.rn=rn; 
 }
 public String toString() {
	 return "Source[this.name="+name+",this.age="+age+",this.rn="+rn+"]";
 }
 public void re() {
	 System.out.println("name"+name);
	 System.out.println(age);
	 System.out.println(rn);
 }
 
 }
public class Mypr1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
Source s1=new Source("srikanth",2,3);
System.out.println(s1.re());
	}

}
